Package: rpdo
Type: Package
Title: Pacific Decadal Oscillation Index Data
Version: 0.3.2
Authors@R: c(
    person("Joe", "Thorley", email = "joe@poissonconsulting.ca", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-7683-4592")),
